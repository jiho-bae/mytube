extends layouts/main
include mixins/videoBlock

block content
    .search__header
        h3 검색어 : 
            span #{searchingBy}
    .search__videos
        if videos.length == 0
            h5 검색 결과가 없습니다.
        each item in videos
            +videoBlock({
                title:item.title,
                views:item.views,
                videoFile:item.fileUrl,
                id:item.id
            })